{
    "Serilog": {
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "System": "Warning",
                "Microsoft": "Warning"
            }
        },
        "Enrich": [ "FromLogContext" ],
        "WriteTo": {
            "Console": {
                "Name": "Console",
                "Args": {
                    "outputTemplate": "[{Level:u3} {Timestamp:HH:mm:ss.fffffffK}] {Scope} {Message:lj}{NewLine}{Exception}"
                }
            }
        }
    },
    "ConnectionStrings": {
        "WebApi": "<enter-connection-string>"
    },
    "Cors": {
        "Origins": [ "*" ]
    },
    "Smtp": {
        "Host": "smtp.mail.ru",
        "Port": 465,
        "UseSSL": true,
        "Username": "<enter-username>",
        "Password": "<enter-password>"
    }
}

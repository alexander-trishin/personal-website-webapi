{
    "name": "personal-website-webapi",
    "version": "1.0.0",
    "private": true,
    "license": "MIT",
    "main": "source/index.ts",
    "cacheDirectories": [
        ".yarn/cache"
    ],
    "scripts": {
        "prepare": "husky install",
        "build": "tsc",
        "clear": "rimraf build coverage",
        "lint": "tsc --noEmit && eslint \"source/**\"",
        "start": "ts-node-dev --files \"./source/index.ts\"",
        "test": "jest"
    },
    "dependencies": {
        "awilix": "5.0.1",
        "fastify": "3.22.0",
        "fastify-awilix": "1.2.0",
        "fastify-env": "2.1.1",
        "fastify-swagger": "4.12.4",
        "nodemailer": "6.6.5"
    },
    "devDependencies": {
        "@types/jest": "27.0.2",
        "@types/node": "16.10.2",
        "@types/nodemailer": "6.4.4",
        "@typescript-eslint/eslint-plugin": "4.32.0",
        "@typescript-eslint/parser": "4.32.0",
        "eslint": "7.32.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-import-resolver-typescript": "2.5.0",
        "eslint-plugin-import": "2.24.2",
        "eslint-plugin-jest": "24.5.0",
        "eslint-plugin-prettier": "4.0.0",
        "husky": "7.0.2",
        "jest": "27.2.4",
        "jest-sonar-reporter": "2.0.0",
        "prettier": "2.4.1",
        "rimraf": "3.0.2",
        "ts-jest": "27.0.5",
        "ts-node": "10.2.1",
        "ts-node-dev": "1.1.8",
        "typescript": "4.4.3"
    },
    "jestSonar": {
        "reportPath": "coverage",
        "reportFile": "sonar.xml",
        "indent": 4
    }
}
